{
  "root": "libs/mex-components",
  "sourceRoot": "libs/mex-components/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/mex-components",
        "tsConfig": "libs/mex-components/tsconfig.lib.json",
        "project": "libs/mex-components/package.json",
        "entryFile": "libs/mex-components/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/mex-components/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/mex-components/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -rf dist",
          "nx run mex-components:build",
          "cp .npmrc dist/libs/mex-components",
          "cd dist/libs/mex-components && npm publish"
        ],
        "parallel": false
      }
    }
  }
}
